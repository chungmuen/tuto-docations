{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": ["src/"],
    "includePattern": "\\.(js)$",
    "excludePattern": "\\.(spec.js)$"
  },
  "recurseDepth": 10,
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs/",
    "template": "./node_modules/docdash"
  },
  "templates": {
    "monospaceLinks": true,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "My code doc",
    "copyright": "DocStrap Copyright Â© 2012-2015 The contributors to the JSDoc3 and DocStrap projects.",
    "theme": "cerulean",
    "linenums": true,
    "inverseNav": true,
    "dateFormat": "ddd MMM Do YYYY"
  }
}